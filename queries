#Obtener todos los compradores
{
  Buyer(func: has(name)) {
   name
   id
   time 
  age
   expand(_all_)
  }
}
#obtener un comprador 
{
	data(func: eq(name, "Leigh")) {
		  name
		  id
		  time
		  age
    }
}
#data

type Buyer {
					id
					name
					age
					products
					time
				}
        type Product{
					id
					name
					price
					time
					buy
				}
        type Transaction{
        		id
            buyer_id
            ip
            device
            products
        }
				id: string @index(term).
        buyer_id: string @index(term).
        ip: string @index(term).
        device: string @index(term).
				name: string @index(term).
				age: int @index(int).
				products:[uid].
				time: int @index(int).
				price: float @index(float). 
				buy: [uid].
#
